@article{RN330,
   author = {Voorter, P. H. M. and Backes, W. H. and Gurney-Champion, O. J. and Wong, S. M. and Staals, J. and van Oostenbrugge, R. J. and van der Thiel, M. M. and Jansen, J. F. A. and Drenthen, G. S.},
   title = {Improving microstructural integrity, interstitial fluid, and blood microcirculation images from multi-b-value diffusion MRI using physics-informed neural networks in cerebrovascular disease},
