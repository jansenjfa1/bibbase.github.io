@article{RN300,
   author = {Canjels, L. P. W. and Ghossein-Doha, C. and Alers, R. J. and Rutten, S. and van den Kerkhof, M. and Schiffer, Vmmm and Mulder, E. and Gerretsen, S. C. and Aldenkamp, A. P. and Hurks, P. P. M. and van de Ven, V. and Spaanderman, M. E. A. and Jansen, J. F. A. and Backes, W. H.},
   title = {Functional connectivity of limbic system years after pre-eclampsia: 7-Tesla functional MRI study},
   journal = {Ultrasound Obstet Gynecol},
   note = {Canjels, L P W
Ghossein-Doha, C
Alers, R-J
Rutten, S
