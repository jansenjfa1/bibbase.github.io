@article{RN193,
   author = {Freeze, W. M. and Schnerr, R. S. and Palm, W. M. and Jansen, J. F. and Jacobs, H. I. and Hoff, E. I. and Verhey, F. R. and Backes, W. H.},
   title = {Pericortical Enhancement on Delayed Postgadolinium Fluid-Attenuated Inversion Recovery Images in Normal Aging, Mild Cognitive Impairment, and Alzheimer Disease},
