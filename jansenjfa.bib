@article{RN363,
   author = {van den Kerkhof, M. and de Jong, J. J. A. and Voorter, P. H. M. and Postma, A. and Kroon, A. A. and van Oostenbrugge, R. J. and Jansen, J. F. A. and Backes, W. H.},
   title = {Blood-Brain Barrier Integrity Decreases With Higher Blood Pressure: A 7T DCE-MRI Study},
   journal = {Hypertension},
   note = {van den Kerkhof, Marieke
de Jong, Joost J A
Voorter, Paulien H M
Postma, Alida
