	keywords = {Female, Humans, Magnetic Resonance Imaging, Brain Mapping, Brain, Child, Epilepsy, Rolandic, Rolandic epilepsy, BOLD time series, discrete wavelet transform, frequency structure, wavelet entropy, Entropy},


