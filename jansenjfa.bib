   url = {https://www.ncbi.nlm.nih.gov/pubmed/37356423},
   year = {2023},
   type = {Journal Article}
}

@article{RN345,
   author = {Monereo-Sanchez, J. and Jansen, J. F. A. and van Boxtel, M. P. J. and Backes, W. H. and Kohler, S. and Stehouwer, C. D. A. and Linden, D. E. J. and Schram, M. T.},
