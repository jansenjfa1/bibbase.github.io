@article{jansen_multimodal_2015,


	title = {Multimodal {MRI} {Reveals} {Secondarily} {Generalized} {Seizure} {Related} {Abnormalities} at 1.5 {T}: {Preliminary} {Findings}},


	volume = {2015},


	issn = {2333-2395},


	url = {https://ibimapublishing.com/articles/JMED/2015/397761/},


	doi = {10.5171/2015.397761},


	abstract = {Patients with chronic epilepsy, who have suffered a high number of secondarily generalized tonicclonic seizures (SGTCS) frequently show cognitive comorbidity. It is yet unclear whether a higher number of SGTCS is associated with tissue changes in the brain. We have investigated in patients with chronic epilepsy whether a high number of SGTCS accumulated over life is associated with microstructural changes in brain tissue. Sixteen patients with localization-related epilepsy with SGTCS underwent a multimodal quantitative Magnetic Resonance (MR) examination at 1.5 T, comprising T2 relaxometry, and diffusion weighted imaging to study microstructural changes in the temporal and frontal lobes. Fourteen healthy volunteers were also included to assess the effect of age. Patients with more than 20 SGTCS (n=8) showed a significantly lower IQ (-20\%, p{\textless}0.05) compared to those with less than 20 SGTCS (n=8). Furthermore, regional combined multimodal analysis revealed that significant quantitative MRI changes, related to the number of SGTCS, were present in the frontal lobe but not in the temporal lobe. Moreover, the left and right frontal lobe generally displayed lower T2 relaxation times, smaller pericortical cerebrospinal fluid fraction and lower apparent diffusion coefficients, in the patients with more than 20 SGTCS. These findings suggest that SGTCS are associated with substantial changes in microstructural brain tissue characteristics within the frontal lobes. These frontal changes possibly explain the cognitive problems which are often observed in patients with many SGTCS. This knowledge may help in the development of treatment aimed at preventing decline in cognitive abilities.},


	journal = {JMED Research},


	author = {Jansen, J. F. and Vlooswijk, M. C. and Reijs, R. P. and Majoie, H. J. and Hofman, P. and Aldenkamp, A. P. and Backes, W. H.},


	year = {2015},


	keywords = {Epilepsy, Cognitive decline, Quantitative MRI, Secondarily generalized tonic-clonic seizures},


	pages = {397761}


}


　


@article{klooster_focal_2018,


	title = {Focal application of accelerated {iTBS} results in global changes in graph measures},


	doi = {10.1002/hbm.24384},


	abstract = {Transcranial magnetic stimulation (TMS) is a non-invasive neurostimulation technique that has shown promising results as treatment for major depressive disorder (MDD). It is assumed that the effects of stimulation are propagated through the brain via anatomical and functional connections. Here, graph analysis was used to study the effect of accelerated intermittent theta burst stimulation (aiTBS) on the brain{\textquoteright}s network topology in medication resistant MDD patients. Anatomical and resting-state functional MRI (rs-fMRI) were recorded at baseline and after sham and verum stimulation. Clinical well-being was assessed using the Hamilton Depression Rating Scale (HDRS). Linear models were fit to investigate the effects of verum versus sham aiTBS. To investigate the potential of graph measures as biomarker, baseline graph measures were correlated with the change in HDRS. In spite that on whole brain level neither significant aiTBS effects, nor biomarker potentials were observed, multiple nodes showed a significant effect of aiTBS after verum aiTBS. Most effects were decreases in graph measures in the hemisphere ipsilateral to the stimulation. Moreover, these effects were found throughout the whole brain and were not restricted to areas that are functionally connected to the stimulation area. Baseline graph measures in two nodes (cingulo-opercular and visuo-temporal region) were correlated with change in HDRS. In conclusion, in contrast to sham stimulation, one week aiTBS results in changes in graph measures in distributed nodes over the brain.},


	journal = {Hum Brain Mapp},


	author = {Klooster, D. C. W. and Franklin, S. L. and Besseling, R. M. and Jansen, J. F. and Caeyenberghs, K. and Duprat, R. and Aldenkamp, A. P. and De Louw, A. J. A. and Boon, P. and Baeken, C.},


	year = {2018},


	keywords = {Graph analysis, Accelerated intermittent theta burst stimulation, Functional connectivity, Transcranial magnetic stimulation}


}


　


