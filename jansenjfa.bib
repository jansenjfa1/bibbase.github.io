@article{RN256,
   author = {Vingerhoets, C. and Tse, D. H. and van Oudenaren, M. and Hernaus, D. and van Duin, E. and Zinkstok, J. and Ramaekers, J. G. and Jansen, J. F. and McAlonan, G. and van Amelsvoort, T.},
   title = {Glutamatergic and GABAergic reactivity and cognition in 22q11.2 deletion syndrome and healthy volunteers: A randomized double-blind 7-Tesla pharmacological MRS study},
