	keywords = {functional MRI, Humans, Magnetic Resonance Imaging, Brain, Cognition Disorders, Adolescent, Nerve Net, Child, cognitive impairment, epilepsy, Epilepsy, Frontal Lobe, functional reorganization, neural networks},


