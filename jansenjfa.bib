@article{RN259,
   author = {Zhou, T. L. and Rensma, S. P. and van der Heide, F. C. T. and Henry, R. M. A. and Kroon, A. A. and Houben, Ajhm and Jansen, J. F. A. and Backes, W. H. and Berendschot, Ttjm and Schouten, Jsag and van Dongen, Mcjm and Eussen, Sjpm and Dagnelie, P. C. and Webers, C. A. B. and Schram, M. T. and Schalkwijk, C. G. and van Sloten, T. T. and Stehouwer, C. D. A.},
   title = {Blood pressure variability and microvascular dysfunction: the Maastricht Study},
   journal = {J Hypertens},
   note = {Zhou, Tan Lai
Rensma, Sytze P
van der Heide, Frank C T
Henry, Ronald M A
Kroon, Abraham A
Houben, Alfons J H M
Jansen, Jacobus F A
