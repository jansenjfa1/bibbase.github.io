@article{RN205,
   author = {Zhang, C. E. and Wong, S. M. and Uiterwijk, R. and Staals, J. and Backes, W. H. and Hoff, E. I. and Schreuder, T. and Jeukens, C. R. and Jansen, J. F. and van Oostenbrugge, R. J.},
   title = {Intravoxel Incoherent Motion Imaging in Small Vessel Disease: Microstructural Integrity and Microvascular Perfusion Related to Cognition},
   journal = {Stroke},
   volume = {48},
   number = {3},
   pages = {658-663},
   note = {Zhang, C Eleana
Wong, Sau May
Uiterwijk, Renske
Staals, Julie
Backes, Walter H
Hoff, Erik I
Schreuder, Tobien
Jeukens, Cecile R L P N
