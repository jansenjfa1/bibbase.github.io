@article{RN309,
   author = {Simons, Pihg and Valkenburg, O. and van de Waarenburg, M. P. H. and van Greevenbroek, M. M. J. and Kooi, M. E. and Jansen, J. F. A. and Schalkwijk, C. G. and Stehouwer, C. D. A. and Brouwers, Mcgj},
   title = {Serum sex hormone-binding globulin is a mediator of the association between intrahepatic lipid content and type 2 diabetes: the Maastricht Study},
   journal = {Diabetologia},
   note = {Simons, Pomme I H G
Valkenburg, Olivier
