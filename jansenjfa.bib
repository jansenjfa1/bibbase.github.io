@article{RN360,
   author = {DeJong, N. R. and Jansen, J. F. A. and van Boxtel, M. P. J. and Schram, M. T. and Stehouwer, C. D. A. and van Greevenbroek, M. M. J. and van der Kallen, C. J. H. and Koster, A. and Eussen, Sjpm and de Galan, B. E. and Backes, W. H. and Kohler, S.},
   title = {Brain structure and connectivity mediate the association between lifestyle and cognition: The Maastricht Study},
   journal = {Brain Commun},
   volume = {6},
   number = {3},
   pages = {fcae171},
   note = {DeJong, Nathan R
Jansen, Jacobus F A
van Boxtel, Martin P J
Schram, Miranda T
