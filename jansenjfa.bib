	number = {8},


	journal = {Investigative Radiology},


	author = {Jansen, Jacobus F. A. and Vlooswijk, Marielle C. G. and Majoie, Hj Marian and de Krom, Marc C. T. F. M. and Aldenkamp, Albert P. and Hofman, Paul A. M. and Backes, Walter H.},


