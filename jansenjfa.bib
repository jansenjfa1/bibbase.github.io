	keywords = {Female, Humans, Male, Magnetic Resonance Spectroscopy, Adult, Cognition, Epilepsy, Neuropsychological Tests, Image Processing, Computer-Assisted, Cognition Disorders, Middle Aged, Diffusion Tensor Imaging, cognition, memory, diffusion tensor imaging, localization-related epilepsy, MR spectroscopy, MRI-negative},


