	pmcid = {PMC5115673},


	keywords = {Female, Humans, Image Interpretation, Computer-Assisted, Magnetic Resonance Imaging, Male, Adult, Brain Mapping, Image Processing, Computer-Assisted, Brain, Malformations of Cortical Development, Case-Control Studies, Young Adult, Rest, Connectome},


