@article{RN133,
   author = {Jansen, J. F. and Vlooswijk, M. C. and Majoie, H. M. and de Krom, M. C. and Aldenkamp, A. P. and Hofman, P. A. and Backes, W. H.},
   title = {White matter lesions in patients with localization-related epilepsy},
