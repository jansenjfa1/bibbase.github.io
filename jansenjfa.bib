@article{RN366,
   author = {Pilmeyer, J. and Lamerichs, R. and Schielen, S. and Ramsaransing, F. and van Kranen-Mastenbroek, V. and Jansen, J. F. A. and Breeuwer, M. and Zinger, S.},
   title = {Multi-modal MRI for objective diagnosis and outcome prediction in depression},
   journal = {Neuroimage Clin},
