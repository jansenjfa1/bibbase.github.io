@article{RN327,
   author = {Veeraiah, P. and Jansen, J. F. A.},
   title = {Multinuclear Magnetic Resonance Spectroscopy at Ultra-High-Field: Assessing Human Cerebral Metabolism in Healthy and Diseased States},
   journal = {Metabolites},
