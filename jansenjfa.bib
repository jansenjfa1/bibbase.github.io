@article{RN357,
   author = {Uher, D. and Drenthen, G. S. and Poser, B. A. and Hofman, P. A. M. and Wagner, L. G. and van Lanen, Rhgj and Hoeberigs, C. M. and Colon, A. J. and Schijns, Oemg and Jansen, J. F. A. and Backes, W. H.},
   title = {DeepFLAIR: A neural network approach to mitigate signal and contrast loss in temporal lobes at 7Tesla FLAIR images},
   journal = {Magn Reson Imaging},
   note = {Uher, Daniel
Drenthen, Gerhard S
Poser, Benedikt A
Hofman, Paul A M
Wagner, Louis G
van Lanen, Rick H G J
Hoeberigs, Christianne M
Colon, Albert J
