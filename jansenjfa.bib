@article{RN257,
   author = {Canjels, L. P. W. and Backes, W. H. and van Veenendaal, T. M. and Vlooswijk, M. C. G. and Hofman, P. A. M. and Aldenkamp, A. P. and Rouhl, R. P. W. and Jansen, J. F. A.},
