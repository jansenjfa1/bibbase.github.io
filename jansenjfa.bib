   DOI = {10.1002/jmri.26920},
   url = {http://www.ncbi.nlm.nih.gov/pubmed/31486211
https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7078988/pdf/JMRI-51-1170.pdf},
   year = {2020},
   type = {Journal Article}
}

