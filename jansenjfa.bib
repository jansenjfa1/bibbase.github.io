	number = {5},


	journal = {International Journal of Radiation Oncology, Biology, Physics},


	author = {Jansen, Jacobus F. A. and Sch{\"o}der, Heiko and Lee, Nancy Y. and Wang, Ya and Pfister, David G. and Fury, Matthew G. and Stambuk, Hilda E. and Humm, John L. and Koutcher, Jason A. and Shukla-Dave, Amita},


