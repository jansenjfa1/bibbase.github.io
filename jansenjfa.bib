	number = {5},


	journal = {Journal of magnetic resonance imaging: JMRI},


	author = {Lu, Yonggang and Jansen, Jacobus F. A. and Mazaheri, Yousef and Stambuk, Hilda E. and Koutcher, Jason A. and Shukla-Dave, Amita},


