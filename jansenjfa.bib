}

@article{RN350,
   author = {de Jong, J. J. A. and Jansen, J. F. A. and Vergoossen, L. W. M. and Schram, M. T. and Stehouwer, C. D. A. and Wildberger, J. E. and Linden, D. E. J. and Backes, W. H.},
   title = {Effect of Magnetic Resonance Image Quality on Structural and Functional Brain Connectivity: The Maastricht Study},
   journal = {Brain Sci},
   volume = {14},
   number = {1},
   note = {de Jong, Joost J A
Jansen, Jacobus F A
Vergoossen, Laura W M
Schram, Miranda T
Stehouwer, Coen D A
Wildberger, Joachim E
Linden, David E J
